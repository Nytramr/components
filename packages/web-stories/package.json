{
  "name": "@nytramr/web-stories",
  "version": "1.0.0",
  "description": "Tool to show and test custom elements in isolation",
  "author": "Martin Rubinsztein <martin.rubinsztein@gmail.com>",
  "homepage": "",
  "license": "MIT",
  "main": "lib/web-stories.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-nytram:Nytramr/components.git"
  },
  "scripts": {
    "build": "npx rollup -c",
    "build:watch": "npx rollup -c --watch",
    "dev": "npx concurrently \"yarn run build:watch\" \"yarn run sass:watch\" \"yarn run serve\"",
    "sass": "npx sass src/css/playground.scss server/css/playground.css",
    "sass:watch": "npx sass --watch src/css/playground.scss server/css/playground.css",
    "serve": "cd server && npx httpserver",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "dependencies": {
    "flux-condenser": "^0.1.1",
    "http-server": "^0.12.3"
  },
  "devDependencies": {
    "@nytramr/nr-iframe": "^0.0.1",
    "@nytramr/nr-tabs": "^0.0.1"
  }
}
